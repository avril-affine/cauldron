{
  "name": "Cauldron",
  "version": "0.0.0",
  "description": "App to annotating PDFs",
  "main": "main_process.js",
  "scripts": {
    "bundle": "webpack --mode development",
    "serve": "electron .",
    "start": "cross-env NODE_ENV=development npm-run-all --parallel bundle serve",
    "flow": "flow",
    "postinstall": "electron-builder install-app-deps",
    "package": "webpack --mode production",
    "release": "electron-builder build --publish never"
  },
  "build": {
    "appId": "com.kkihara.cauldron",
    "files": [
      "app/build/*",
      "node_modules/**/*",
      "main_process.js",
      "app/index.html",
      "package.json"
    ]
  },
  "author": "Kenneth Kihara",
  "repository": "kkihara/cauldron",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.10",
    "electron": "^1.8.2",
    "electron-builder": "^20.38.4",
    "electron-rebuild": "^1.8.2",
    "electron-reload": "^1.2.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.10",
    "flow-bin": "^0.83.0",
    "node-loader": "^0.6.0",
    "node-native-loader": "^1.1.1",
    "npm-run-all": "^4.1.5",
    "resolve-url-loader": "^3.0.0",
    "webpack": "^4.1.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "fuse.js": "^3.3.0",
    "js-yaml": "^3.12.1",
    "moment": "^2.23.0",
    "node": "^11.5.0",
    "node-gyp": "^3.8.0",
    "pdfjs-dist": "^2.0.943",
    "rangy": "^1.3.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-split-pane": "^0.1.84",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sqlite3": "^4.0.4",
    "styled-components": "^4.1.3"
  }
}
